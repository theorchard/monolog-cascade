{
    "name": "theorchard/monolog-cascade",
    "license": "MIT",
    "type": "library",
    "description": "Monolog extension to configure multiple loggers in the blink of an eye and access them from anywhere",
    "keywords": ["monolog", "cascade", "monolog extension", "monolog plugin", "monolog utils", "logger", "log"],
    "authors": [
        {
            "name": "RaphaÃ«l Antonmattei",
            "email": "rantonmattei@theorchard.com",
            "homepage": "https://github.com/rantonmattei"
        }
    ],
    "homepage": "https://github.com/theorchard/monolog-cascade",
    "require": {
        "php": ">=5.3.9",
        "symfony/config": "~2.7.0",
        "symfony/options-resolver": "~2.7.0",
        "symfony/serializer": "~2.7.0",
        "symfony/yaml": "~2.7.0",
        "monolog/monolog": "~1.14"
    },
    "autoload": {
        "psr-4": {
            "Cascade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cascade\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.4",
        "phpunit/phpcov": "~2.0",
        "phpunit/phpunit": "~4.7",
        "phpunit/phpunit-mock-objects": "2.3.0",
        "satooshi/php-coveralls": "~0.6",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "_": "phpunit/phpunit-mock-objects required in 2.3.0 due to https://github.com/sebastianbergmann/phpunit-mock-objects/issues/223",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
